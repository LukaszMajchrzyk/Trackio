<Page x:Class="Trackio.View.PageOpenProject"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Trackio.View"
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="900"
      Title="PageBlank">

    <Grid Background="#FFD0D0D0">
        <DataGrid x:Name="datagridIDsAndProjectsNames" AutoGeneratedColumns="dataGridHeaderNames" MinColumnWidth="40"  ColumnWidth="*" Margin="10,10,10,104" SelectionChanged="DataGrid_SelectionChanged" SelectionMode="Single"/>
        <Button x:Name="buttonOpen" Content="Open" HorizontalAlignment="Left" Margin="716,451,0,0" VerticalAlignment="Top" Width="74" Click="buttonOpenClick"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="802,451,0,0" VerticalAlignment="Top" Width="74" Click="buttonCancel_Click" />
        <Frame x:Name="frameOpenProject" NavigationUIVisibility ="Hidden" Width="900"/>


    </Grid>
</Page>
